<li class="hidden">
	<a href="/"></a>
</li>
{% if theme.theme_mods.menu_services %}
	<li class="menu-item-has-children">
		<a class="page-scroll" href="{{ menu_slash }}#services">
			{{__('Services', domain)}}
		</a>
		{% if  service_items %}
			<ul>
				{% for item in service_items %}
					<li>
						<a href="{{ item | permalink }}">
							{{ item.menu_title }}
							{% if not item.menu_title %}
								{{item.post_title}}
							{% endif %}
						</a>
					</li>
				{% endfor %}
			</ul>
		{% endif %}
	</li>
{% endif %}
{% if theme.theme_mods.menu_portfolio %}
	<li class="menu-item-has-children">
		<a class="page-scroll" href="{{ menu_slash }}#portfolio">
			{{__('Portfolio', domain)}}
		</a>
		{% if  portfolio_items %}
			<ul>
			{% for item in portfolio_items %}
				<li>
					<a href="{{ item | permalink }}">
						{{ item.menu_title }}
						{% if not item.menu_title %}
							{{item.post_title}}
						{% endif %}
					</a>
				</li>
			{% endfor %}
			</ul>
		{% endif %}
	</li>
{% endif %}
{% if theme.theme_mods.menu_team %}
	<li>
		<a class="page-scroll" href="{{ menu_slash }}#team">
			{{__('Team', domain)}}
		</a>
	</li>
{% endif %}
{% if theme.theme_mods.menu_contact %}
	<li>
		<a class="page-scroll" href="{{ menu_slash }}#contact">
			{{__('Contact', domain)}}
		</a>
	</li>
{% endif %}
